@model ForgetPasswordViewModel



<div class="container mt-3" style="margin-top:200px;"> 
    <form method="post">
        <div asp-validation-summary="All" class="text-danger"></div>
      <div class="mt-3">
            <label asp-for="Password">Password: </label>
            <input asp-for="Password" class="form-control" />
      </div>
        <div class="mt-3">
            <label asp-for="ConfirmPassword">ConfirmPassword: </label>
            <input asp-for="ConfirmPassword" class="form-control" />
        </div> <br />
        <button type="submit" class="btn btn-outline-dark">Reset Password</button> <br /> <br />
    </form>
</div>